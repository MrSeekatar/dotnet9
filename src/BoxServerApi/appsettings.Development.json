{
  // values for local dev testing
  "KESTREL": {
    "UseLocalSsl": true,
    "SslPort": 44300,
    "CertFileName": "../../../cert.pfx"
  },
  "KeyVaultName": "", // set to use Azure Key Vault
  "Serilog": {
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.IdentityModel.LoggingExtensions.IdentityLoggerAdapter": "Warning"
      }
    },
    "Properties": {
      "Application": "BoxServer"
    }
    // Uncomment out disable logging to remote
    // ,
    // "WriteTo": [
    //   {
    //     "Name": "NullSink"
    //   }
    // ]
  },

  "Telemetry": {
    "Disable": false,
    "EnableDebugTrace": true // set to true to see debug traces from Application Insights in the Rider plugin
  },
  "TemplateName": "BoxServer"
}
